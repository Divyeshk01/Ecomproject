<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.thedstore">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/shoppingcartmodified"
        android:label="@string/app_name"
        android:roundIcon="@drawable/shoppingcart"
        android:supportsRtl="true"
        android:theme="@style/Theme.TheDstore"
        tools:targetApi="31">
        <activity
            android:name=".category_details"
            android:exported="false" />
        <activity
            android:name=".detailmyorder"
            android:exported="false" />
        
        <activity
            android:name=".Orderpage"
            android:exported="false" />
        <activity
            android:name=".Product_fullview"
            android:exported="false" />
        <activity
            android:name=".Home"
            android:exported="false" />
        <activity
            android:name=".RegisterActivity"
            android:exported="false" />
        <activity
            android:name=".LoginActivity"
            android:exported="false" />
        <activity
            android:name=".Firstscreen"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>